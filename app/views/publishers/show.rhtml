<label for="publisher_region">Region</label>
<div class="reviewfield"><%= @publisher.region  %></div><br />

<label for="publisher_postal_code">Postal code</label>
<div class="reviewfield"><%= @publisher.postal_code  %></div><br />

<label for="publisher_country">Country</label>
<div class="reviewfield"><%= @publisher.country  %></div><br />

<label for="publisher_phone">Phone</label>
<div class="reviewfield"><%= @publisher.phone  %></div><br />

<label for="publisher_fax">Fax</label>
<div class="reviewfield"><%= @publisher.fax  %></div><br />

<label for="publisher_email">Email</label>
<div class="reviewfield"><%= @publisher.email  %></div><br />

<label for="publisher_web_site">Web site</label>
<div class="reviewfield"><%= @publisher.web_site  %></div><br />

<label for="publisher_note">Note</label>
<div class="reviewfield"><%= @publisher.note  %></div><br />

<%= link_to 'Edit', :action => 'edit', :id => @publisher %> |
<%= link_to 'Back', :action => 'list' %>
</div> <!-- end of div.editform -->