<div class="editform">
	<h2>Display Publisher: <%= @publisher.publisher_name %></h2>
<div id="form_cols">
<!--[form:publisher]-->
<label for="publisher_publisher_name">Publisher name</label>
<div class="reviewfield"><%= @publisher.publisher_name  %></div><br />

<label for="publisher_publisher_code">Publisher code</label>
<div class="reviewfield"><%= @publisher.publisher_code  %></div><br />

<label for="publisher_address_line_1">Address line 1</label>
<div class="reviewfield"><%= @publisher.address_line_1  %></div><br />

<label for="publisher_address_line_2">Address line 2</label>
<div class="reviewfield"><%= @publisher.address_line_2  %></div><br />

<label for="publisher_city">City</label>
